{
  "name": "english-learning-app",
  "version": "1.0.0",
  "description": "An English learning app built with Electron, React 19, and TypeScript",
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "pnpm run dev:vite",
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "dev:full": "concurrently \"pnpm run dev:vite\" \"pnpm run dev:electron\"",
    "build": "vite build",
    "build:electron": "tsc -p electron",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "electron . --dev",
    "clean": "pnpm store prune"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "electron": "^27.0.0"
  },
  "keywords": ["electron", "react", "typescript", "english", "learning", "education"],
  "author": "English Learning App Team",
  "license": "MIT"
}
